<h1>Log In to Actualize Overflow</h1>

<%= form_tag '/login', method: :post do %>
  <div>
    <%= label_tag :email %> 
    <%= email_field_tag :email %>    
  </div>
  <div>
    <%= label_tag :password %> 
    <%= password_field_tag :password %>    
  </div>
  <%= submit_tag 'Log Me In' %>
<% end %>