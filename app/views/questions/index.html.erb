<div class="row"> 
  <% @questions.each do |question| %>
    <div class="thumbnail well">        
      <h4><a href="/questions/<%= question.id %>"><%= question.title %></a></h4>
      <h5><%= question.problem %></h5>
      <% if question.status == "Answered" %>
        <p>Resolution: <%= question.resolution %></p>
      <% end %>
      <p>Status: <%= question.status %> </p>
      <p>Number of Suggestions: <%= question.answers.count %></p>
    </div>
  <% end %>
</div>